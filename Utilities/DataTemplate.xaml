<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
                    xmlns:view="clr-namespace:WPFDeskManager.Views"
                    xmlns:vm="clr-namespace:WPFDeskManager.ViewModels"
                    >

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ItemsVM}">
        <view:Items />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:BuildingsVM}">
        <view:Buildings />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:DeskStatusesVM}">
        <view:DeskStatuses />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:DesksVM}">
        <view:Desks />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:FloorsVM}">
        <view:Floors />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:UsersVM}">
        <view:Users />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:DeleteEntityVM}">
        <view:DeleteEntity />
    </DataTemplate>

</ResourceDictionary>