<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--#region Button Colors-->

    <Color x:Key="ButtonBlueColor">#0471A6</Color>
    <Color x:Key="ButtonBlueHoverColor">#058ACB</Color>
    <Color x:Key="ButtonTextColor">#DEDEDE</Color>

    <Color x:Key="DeleteButtonColor">#F32525</Color>
    <Color x:Key="DeleteButtonHoverColor">#BD0303</Color> 

    <Color x:Key="EditButtonColor">#EFCA08</Color>
    <Color x:Key="EditButtonHoverColor">#F49F0A</Color>
    
    <Color x:Key="ShowButtonColor">#85C15D</Color>
    <Color x:Key="ShowButtonHoverColor">#19B244</Color>

    <Color x:Key="ActiveBarColor">#D9D9D9</Color>

    <!--#endregion-->
    
    <!--#region Background Colors-->
    
    <Color x:Key="BGMainColor">#D9D9D9</Color>

    <Color x:Key="BGSideBarColor">#0471A6</Color>

    <!--#endregion-->

    <!--#region Table Colors-->

    <Color x:Key="TableColumnHeaderBGColor">Transparent</Color>
    <Color x:Key="TableColumnHeaderTextColor">#0471A6</Color>

    <Color x:Key="TableRowBGColor">#FFFFFF</Color>
    <Color x:Key="TableRowBGHoverColor">#0471A6</Color>

    <Color x:Key="TableRowTextHoverColor">#FFFFFF</Color>
    <Color x:Key="TableRowTextColor">#000000</Color>

    <!--#endregion-->
    
</ResourceDictionary>